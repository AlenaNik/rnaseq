nextflow_pipeline {

    name "Test pipeline with min_mapped_reads = 90"
    script "../main.nf"
    tag "rnaseq"
    tag "PIPELINE"

    test("--min_mapped_reads 90") {

        when {
            params {
                outdir           = "$outputDir"
                min_mapped_reads = 90
            }
        }

        then {
            assert workflow.success
        }
    }
}
