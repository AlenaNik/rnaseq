nextflow_pipeline {

    name "Test pipeline with --skip_trimming"
    script "../main.nf"
    tag "RNASEQ"
    tag "PIPELINE"

    test("Parameters: --skip_trimming") {

        when {
            params {
                outdir = "$outputDir"
                skip_trimming = true
            }
        }

        then {
            assert workflow.success
        }
    }
}
